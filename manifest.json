{
  "manifest_version": 2,
  "name": "RayAI Screen Reader",
  "short_name": "RayAI",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://www.googleapis.com/*"
  ]
}